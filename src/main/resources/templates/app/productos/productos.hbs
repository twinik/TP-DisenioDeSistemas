                        <p class="mb-3 text-xl font-normal text-white">
                            {{this.puntosNecesarios}}
                        </p>
                        <a href="#" class="inline-flex items-center boton">
                            Canjear
                        </a>
                    </div>
                </div>
            {{/each}}

